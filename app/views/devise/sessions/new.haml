.login-box
  .login-logo
    %a{:href => "../../index2.html"}
      %b> User Login
  / /.login-logo
  = render '/admin/shared/flash_messages'
  = devise_error_messages!
  .login-box-body
    %p.login-box-msg Sign in to start your session
    = form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
      .form-group.has-feedback
        = f.email_field :email, autofocus: true, "class" => 'form-control', :placeholder => "Email", required: true
        %span.glyphicon.glyphicon-envelope.form-control-feedback
      .form-group.has-feedback
        = f.password_field :password, autocomplete: "off", class: 'form-control', :placeholder => "Password", required: true
        %span.glyphicon.glyphicon-lock.form-control-feedback
      .row
        .col-xs-4
          %button.btn.btn-primary.btn-block.btn-flat{:type => "submit"} Sign In
        / /.col
  / /.login-box-body
/ /.login-box