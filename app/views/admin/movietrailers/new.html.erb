 <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            New MovieTrailer Form 
            <small>Add new movietrailer</small>
          </h1>
          <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li><a href="#">Forms</a></li>
            <li class="active">General Elements</li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">
          <div class="row">
            <!-- left column -->
            <div class="col-md-12">
              <!-- general form elements -->
              <div class="box box-primary">
                <div class="box-header with-border">
                  <h3 class="box-title">Add New MovieTrailor</h3>
                </div><!-- /.box-header -->
                <!-- form start -->
                <form role="form">
                  <div class="box-body">
                    <div class="form-group">
                      <label for="desc">Descripton</label>
                      <input type="text" class="form-control" id="desc" placeholder="Description for movietrailer">
                    </div>

                    <div class="form-group">
                      <label for="title">Title</label>
                      <input type="text" class="form-control" id="title" placeholder="Title for movietrailer">
                    </div>

                    <div class="form-group">
                      <label for="published_at">Published_date</label>
                      <input type="date" class="form-control" id="published_at" placeholder="Published Date">
                    </div>

                    <div class="form-group">
                      <label for="total_view">Total_View</label>
                      <input type="integer" class="form-control" id="total_view" placeholder="Totally Viewed by Users">
                    </div>

                    <div class="form-group">
                      <label for="category_id">Category_id</label>
                      <input type="text" class="form-control" id="category_id">
                    </div>


                  </div><!-- /.box-body -->

                  <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Submit</button>
